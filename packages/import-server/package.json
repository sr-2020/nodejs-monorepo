{
  "name": "import-server",
  "version": "1.0.0",
  "description": "Import server part",
  "main": "./dist/server.js",
  "scripts": {
    "deps": "yarn install",
    "test": "mocha",
    "coverage:test": "nyc --cwd=.. --temp-dir=import-server/.nyc_output mocha",
    "coverage:merge": "nyc merge .nyc_output ../../.nyc_output/import-server.coverage.json",
    "test:watch": "chokidar src/**/* test/**/* -c \"npm test\"",
    "start": "node -r tsconfig-paths/register ./src/beta-testers-creator.js",
    "stop": "forever stop ./dist/server.js",
    "dev": "tsc && node ./dist/server.js",
    "build": "tsc -b",
    "build:live": "set NODE_ENV=develop && nodemon --config nodemon.json",
    "reexport": "tsc && node ./dist/server.js -- --export",
    "reimport": "tsc && node ./dist/server.js -- --import",
    "debug": "tsc && node --inspect-brk=127.0.0.1:9229 ./dist/server.js -- --mail --id=10643",
    "import:tables": "ts-node src/tables-import/tables-importer.ts",
    "import:test": "tsc && node ./dist/server.js -- --test",
    "import:db": "ts-node src/db-docs-creator.ts",
    "processor": "ts-node src/processors/index.ts",
    "lint": "yarn run prettier:check && yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "prettier --ignore-path=../../.prettierignore  \"**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint -c ../../.eslintrc.js --ignore-path=../../.eslintignore --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@types/command-line-args": "^4.0.0",
    "@types/dotenv": "^4.0.3",
    "@types/pouchdb": "^6.1.1",
    "@types/pouchdb-find": "^6.3.0",
    "@types/request-promise-native": "^1.0.6",
    "@types/winston": "^2.3.3",
    "command-line-args": "^4.0.6",
    "command-line-usage": "^4.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.15.3",
    "google-auth-library": "^0.10.0",
    "googleapis": "^59.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.18.1",
    "pouchdb": "^7.1.1",
    "pouchdb-find": "^6.3.4",
    "pouchdb-upsert": "^2.2.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "rxjs": "^5.4.2",
    "winston": "^2.3.1",
    "winston-elasticsearch": "^0.5.8"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/command-line-usage": "^5.0.1",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.116",
    "@types/pouchdb-upsert": "^2.2.4",
    "chai": "^4.1.0",
    "nodemon": "^1.11.0"
  }
}
